{
  "branchPrefix": "renovate/",
  "username": "thiagolunardi",
  "gitAuthor": "Thiago Lunardi <thiago@thiagolunardi.net>",
  "platform": "github",
  "onboarding": false,
  "requireConfig": "optional",
  "forkProcessing": "enabled",
  "repositories": [
    "desenvolvedor-io/dev-store"
  ],
  "enabledManagers": [
    "nuget"
  ],
  "packageRules": [
    {
      "description": "Microsoft packages",
      "matchManagers": [
        "nuget"
      ],
      "matchPackageNames": [
        "AspNetCore.*",
        "Microsoft.*",
        "System.*"
      ],
      "dependencyDashboardApproval": false,
      "minimumReleaseAge": "2 days"
    },    
    {
      "description": "External packages",
      "matchManagers": [
        "nuget"
      ],
      "matchPackageNames": [
        "!AspNetCore.*",
        "!Microsoft.*",
        "!System.*"
      ],
      "dependencyDashboardApproval": false,
      "minimumReleaseAge": "7 days"
    }

  ]
}